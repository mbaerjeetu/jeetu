pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    }
    
    stage {'Upload Config Data'} {
    Steps {
    componante
    sh "echo Validating config file {$configfilepath} "
    script {
    ChangeSetId=snDevOpsconfigUpload(
    applicationName: 'Jenkins 101'
     uploadTarget: 'component'
     namePath: 'wep-api-v1.0',
     configFilePath: '**/*.param.json',
     convertpath:'true',
     dataFormat: 'json',
     autoCommit: 'true'​
     autoValidate: 'false'​
     changesetNumber: '$(componentUpload.changesetNumber)'
    )
    snDevOpsconfigvalidate(
    applicationName:'Jenkins 101'
    deployableName: 'production'
    snapShotName: 'Production_1-v1.dpl'
    )
    snDevOpsconfigGetSnapshots(
    
    applicationName:'Jenkins 101'
    deployableName: 'production'
    isValidate:'true'
    )
    
    snDevOpsconfigPublish(
        applicationName:'Jenkins 101'
    deployableName: 'production'
    snapShotName: 'Production_1-v1.dpl'
}
